{
  "iisSettings": {
    "windowsAuthentication": false,
    "anonymousAuthentication": true,
    "iis": {
      "applicationUrl": "http://localhost/Pandora.NetCore.Identity",
      "sslPort": 0
    },
    "iisExpress": {
      "applicationUrl": "http://localhost:5010",
      "sslPort": 44366
    }
  },
  "$schema": "http://json.schemastore.org/launchsettings.json",
  "profiles": {
    "IIS Express": {
      "commandName": "IISExpress",
      "launchUrl": "auth/v1/auth",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      }
    },
    "Pandora.NetCore.Identity": {
      "commandName": "IISExpress",
      "launchUrl": "auth/v1/auth",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      },
      "applicationUrl": "https://localhost:5001;http://localhost:5000"
    },
    "Docker": {
      "commandName": "Docker",
      "launchBrowser": true,
      "launchUrl": "{Scheme}://{ServiceHost}:{ServicePort}/auth/v1/auth",
      "httpPort": 50303,
      "useSSL": true,
      "sslPort": 44367
    }
  }
}